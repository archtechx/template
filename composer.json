{
    "name": "archtechx/replace",
    "description": "",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Å tancl",
            "email": "samuel@archte.ch"
        }
    ],
    "autoload": {
        "psr-4": {
            "ArchTech\\REPLACE\\": "src/"
        }
    },
    "autoload-dev": {
